<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gray shadow. -->
    <item android:left="0dp" android:bottom="2dp">
        <shape>
            <solid android:color="#226BE8" />
            <corners android:radius="0dp" />
        </shape>
    </item>
    <!-- White foreground. -->
    <item android:top="2dp" android:right="0dp">
        <shape>
            <solid android:color="@color/color_dark_blue" />
            <corners android:radius="0dp" />
        </shape>
    </item>
</layer-list>